<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmartSearchView">
        <attr name="smsv_searchBarWidth" format="dimension"/>
        <attr name="smsv_searchBarMarginTop" format="dimension"/>
        <attr name="smsv_searchBarMarginLeft" format="dimension"/>
        <attr name="smsv_searchBarMarginRight" format="dimension"/>
        <attr name="smsv_searchSuggestionTextSize" format="dimension"/>
        <attr name="smsv_searchInputTextSize" format="dimension"/>
        <attr name="smsv_showMenuAction" format="boolean"/>
        <attr name="smsv_dismissFocusOnItemSelection" format="boolean"/>
        <attr name="smsv_leftActionMode">
            <enum name="showHamburger" value="1" />
            <enum name="showSearch" value="2" />
            <enum name="showHome" value="3" />
            <enum name="noLeftAction" value="4" />
        </attr>
        <attr name="smsv_showVoiceInput" format="boolean"/>
        <attr name="smsv_showSearchKey" format="boolean"/>
        <attr name="smsv_voiceRecHint" format="string"/>
        <attr name="smsv_searchHint" format="string"/>
        <attr name="smsv_dismissOnOutsideTouch" format="boolean"/>
        <attr name="smsv_showMoveSuggestionUp" format="boolean"/>
        <attr name="smsv_suggestionsListAnimDuration" format="integer"/>
        <attr name="smsv_hideOverflowMenuWhenFocused" format="boolean"/>
        <attr name="smsv_showOverFlowMenu" format="boolean"/>
        <attr name="smsv_close_search_on_keyboard_dismiss" format="boolean"/>
        <attr name="smsv_menu" format="reference"/>
        <attr name="smsv_leftActionColor" format="color"/>
        <attr name="smsv_viewTextColor" format="color"/>
        <attr name="smsv_viewSearchInputTextColor" format="color"/>
        <attr name="smsv_hintTextColor" format="color"/>
        <attr name="smsv_viewSuggestionItemTextColor" format="color"/>
        <attr name="smsv_menuItemIconColor" format="color"/>
        <attr name="smsv_actionMenuOverflowColor" format="color"/>
        <attr name="smsv_clearBtnColor" format="color"/>
        <attr name="smsv_backgroundColor" format="color"/>
        <attr name="smsv_dividerColor" format="color"/>
        <attr name="smsv_dimBackground" format="boolean"/>
        <attr name="smsv_suggestionRightIconColor" format="color"/>
    </declare-styleable>
</resources>